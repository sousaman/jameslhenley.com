<div class="container-fluid p-0">

    {{!-- Cover --}}
    <section class="resume-section p-3 p-lg-5 d-flex d-column" id="about">
        <div class="my-auto">
            <h1 class="mb-0">James
                <span class="text-primary">Henley</span>
            </h1>
            {{#each data.general}}
            <div class="subheading mb-5">Email
                <a href="mailto:{{contactEmail}}">{{contactEmail}}</a>
            </div>
            <p class="lead mb-5">{{intro}}</p>
            {{/each}}
            <div class="social-icons">
                <a href="https://www.linkedin.com/in/jameshenley1/" target="_blank">
                    <i class="fab fa-linkedin-in"></i>
                </a>
                <a href="https://github.com/sousaman" target="_blank">
                    <i class="fab fa-github"></i>
                </a>
                <a href="https://www.instagram.com/james.l.henley/" target="_blank">
                    <i class="fab fa-instagram"></i>
                </a>
            </div>
        </div>
    </section>

    <hr class="m-0">


    {{!-- Experience Section --}}
    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="experience">
        <div class="my-auto">
            <h2 class="mb-5">Experience</h2>

            {{#each data.professions}}
            <div class="resume-item d-flex flex-column flex-md-row mb-5">
                <div class="resume-content mr-auto">
                    <h3 class="mb-0">{{jobTitle}}</h3>
                    <div class="subheading mb-3"><a href="{{websiteLink}}" target="_blank">{{company}}</a></div>
                    <p>{{{responsibilities}}}</p>
                </div>
                <div class="resume-date text-md-right">
                    {{#if endDate}}
                    <span class="subheading text-primary">{{startDate}} - {{endDate}}</span>
                    {{/if}}
                    {{#unless endDate}}
                    <span class="subheading text-primary">{{startDate}} - Present</span>
                    {{/unless}}
                </div>
            </div>
            {{/each}}

        </div>

    </section>

    <hr class="m-0">


    {{!-- Education --}}
    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="education">
        <div class="my-auto">
            <h2 class="mb-5">Education</h2>

            {{#each data.education}}
            <div class="resume-item d-flex flex-column flex-md-row mb-5">
                <div class="resume-content mr-auto">
                    <h3 class="mb-0"><a class="text-secondary" href="{{websiteLink}}">{{school}}</a></h3>
                    <div class="subheading text-primary mb-3">{{degree}}</div>
                    {{!-- {{#each minors}}
                    <div class="text-primary mb-3">{{this}}</li></div>
                    {{/each}} --}}
                </div>
                <div class="resume-date text-md-right">
                    <span class="subheading text-primary">{{gradDate}}</span>
                </div>
            </div>
            {{/each}}

        </div>
    </section>

    <hr class="m-0">


    {{!-- Certifications --}}
    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="certifications">
        <div class="my-auto">
            <h2 class="mb-5">Certifications</h2>
                {{#each data.certifications}}
                <div class="resume-item d-flex flex-column flex-md-row mb-5">
                    <div class="resume-content mr-auto">
                        <h3 class="mb-0"><a class="text-secondary" href="{{entityLink}}">{{entity}}</a></h3>
                        {{#each certification}}
                        <div class="subheading text-primary mb-3">{{this}}</div>
                        {{/each}}
                        {{!-- {{#each minors}}
                        <div class="text-primary mb-3">{{this}}</li></div>
                        {{/each}} --}}
                    </div>
                    <div class="resume-date text-md-right">
                        <div class="subheading text-primary">{{denotation}}</div>
                        <div class="subheading text-primary">{{shortDenote}}: {{startDenote}} - {{endDenote}}</div>
                    </div>
                </div>
                {{/each}}
        </div>
    </section>


    <hr class="m-0">


    {{!-- Skills --}}
    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="skills">
        <div class="my-auto">
            <h2 class="mb-5">Skills</h2>

            <div class="subheading mb-3">Programming Languages &amp; Tools</div>
            <ul class="list-inline dev-icons">
                <li class="list-inline-item">
                    <i class="fab fa-html5"></i>
                </li>
                <li class="list-inline-item">
                    <i class="fab fa-css3-alt"></i>
                </li>
                <li class="list-inline-item">
                    <i class="fab fa-js-square"></i>
                </li>
                {{!-- <li class="list-inline-item">
                    <i class="fab fa-react"></i>
                </li> --}}
                <li class="list-inline-item">
                    <i class="fab fa-node-js"></i>
                </li>
                <li class="list-inline-item">
                    <i class="fab fa-npm"></i>
                </li>
            </ul>

            {{!-- <div class="subheading mb-3">Workflow</div>
            <ul class="fa-ul mb-0">
                <li>
                    <i class="fa-li fa fa-check"></i>
                    Mobile-First, Responsive Design</li>
                <li>
                    <i class="fa-li fa fa-check"></i>
                    Cross Browser Testing &amp; Debugging</li>
            </ul> --}}
        </div>
    </section>

    
    <hr class="m-0">


    {{!-- Interests --}}
    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="interests">
        <div class="my-auto">
            <h2 class="mb-5">Interests <br>(Click each interest for my latest developments in each)</h2>
            {{#each data.interests}}
            <p><a class="text-secondary subheading" href="{{link}}" target="_blank">{{interest}}</a></p>
            {{/each}}
            {{!-- <p class="mb-0">Lorem ipsum dolor sit amet</p> --}}
        </div>
    </section>

</div>

<!-- Bootstrap core JavaScript -->
<script src="/public/js/jquery.min.js"></script>
<script src="/public/js/bootstrap.bundle.min.js"></script>

<!-- Plugin JavaScript -->
<script src="/public/js/jquery.easing.min.js"></script>

<!-- Custom scripts -->
<script src="/public/js/resume.js"></script>